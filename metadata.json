{
  "name":         "opentable-puppetversion",
  "version":      "1.0.0",
  "author":       "opentable",
  "license":      "MIT",
  "summary":      "Module for managing the installation and upgrade of puppet",
  "source":       "https://github.com/opentable/puppet-puppetversion",
  "project_page": "https://github.com/opentable/puppet-puppetversion",
  "issues_url":   "https://github.com/opentable/puppet-puppetversion/issues",
  "tags": ["windows","redhat","debian","puppet", "upgrades"],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": ["5.0","6.0"]
    },
    { "operatingsystem": "CentOS",
      "operatingsystemrelease": ["5.0","6.0"]
    },
    { "operatingsystem": "Debian",
      "operatingsystemrelease": ["6.0","7.0"]
    },
    { "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["12.04","14.04"]
    }
  ],
  "dependencies": [
    {
       "name": "puppetlabs/stdlib",
       "version_requirement": ">= 3.0.0 <5.0.0"
    },
    {
       "name": "puppetlabs/apt",
       "version_requirement": ">= 1.4.0 <2.0.0"
    },
    {
       "name": "puppetlabs/inifile",
       "version_requirement": ">= 1.0.0 <2.0.0"
    },
    {
       "name": "stahnma/puppetlabs_yum",
       "version_requirement": ">= 0.1.4 <1.0.0"
    },
    {
       "name": "opentable/altlib",
       "version_requirement": ">= 0.2.0 <1.0.0"
    }
  ]
}
